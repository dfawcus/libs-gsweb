#include "Base"

ACheckBoxList: GSWCheckBoxList
{
        list = application.bundleInfo.CheckBoxList.list;
        item = anItem;
        value = anItem.value;
        displayString = anItem.label;
        selections = selectedItems;
        name = "CHECKNAME";
};

SelectedItems: GSWString
{
        value = selectedItems;
};

SelectedValues: GSWString
{
        value = selectedValues;
};

