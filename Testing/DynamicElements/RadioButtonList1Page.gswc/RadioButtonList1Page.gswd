#include "Base"

ARadioButtonList: GSWRadioButtonList
{
        list = application.bundleInfo.RadioButtonList.list;
        item = anItem;
        value = anItem.value;
        displayString = anItem.label;
        selection = selectedItem;
        name = "RADIONAME";
};

SelectedItem: GSWString
{
        value = selectedItem;
};

